<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- mapper는 여러 개가 존재할 수 있으니 다른 maper아 중복이 되지 않도록 namespace를 선언 -->
<mapper namespace="plMap">
	<select id="getMemberPLList" parameterType="String"
		resultType="com.company.dto.PayLogDTO">
		SELECT * FROM pay_log WHERE M_ID LIKE #{mId} ORDER BY PL_DATE1 DESC, PL_DATE2 DESC, PL_DATE3 DESC;
	</select>
</mapper>